<?xml version="1.0" ?>
<robot name="Root">
   <link name="Base">
      <inertial>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <mass value="1.0"/>
         <inertia ixx="0.1" iyy="0.1" izz="0.1" ixy="0" ixz="0" iyz="0"/>
      </inertial>
   </link>
   <link name="WallLeft">
      <visual>
         <geometry>
            <box size="0.1 1.0 2.05"/>
         </geometry>
         <material name="gray"/>
         <origin xyz="0 -0.52 1.025" rpy="0 0 0"/>
      </visual>
      <!-- <collision>
         <geometry>
            <box size="0.1 1.0 2.05"/>
         </geometry>
         <origin xyz="0 -0.52 1.025" rpy="0 0 0"/>
      </collision> -->
   </link>
   <link name="WallRight">
      <visual>
         <geometry>
            <box size="0.1 1.0 2.05"/>
         </geometry>
         <material name="gray"/>
         <origin xyz="0 0.52 1.025" rpy="0 0 0"/>
      </visual>
      <!-- <collision>
         <geometry>
            <box size="0.1 1.0 2.05"/>
         </geometry>
         <origin xyz="0 0.52 1.025" rpy="0 0 0"/>
      </collision> -->
   </link>
   <link name="Door">
      <inertial>
         <origin xyz="0.0 -0.5 1.0" rpy="0.0 0.0 0.0"/>
         <mass value="20.0"/>
         <inertia ixx="8.333" iyy="6.667" izz="1.667" ixy="0" ixz="0" iyz="0"/>
      </inertial>
      <visual>
         <geometry>
            <box size="0.04 1.0 1.95"/>
         </geometry>
         <material name="brown"/>
         <origin xyz="0.0 -0.5 1.0" rpy="0 0 0"/>
      </visual>
      <collision>
         <geometry>
            <box size="0.04 1.0 1.95"/>
         </geometry>
         <origin xyz="0.0 -0.5 1.0" rpy="0 0 0"/>
      </collision>
   </link>
   <joint type="fixed" name="wall_left_joint">
      <parent link="Base"/>
      <child link="WallLeft"/>
      <origin xyz="0 -0.5 0" rpy="0 0 0"/>
   </joint>
   <joint type="fixed" name="wall_right_joint">
      <parent link="Base"/>
      <child link="WallRight"/>
      <origin xyz="0 0.5 0" rpy="0 0 0"/>
   </joint>
   <joint type="revolute" name="door_joint">
      <parent link="WallRight"/>
      <child link="Door"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <dynamics damping="2.0" friction="0.0"/>
      <axis xyz="0.0 0.0 1.0"/>
      <limit lower="-2.618" upper="0" effort="100" velocity="100"/>
   </joint>
   <material name="gray">
      <color rgba="0.6 0.6 0.6 1"/>
   </material>
   <material name="brown">
      <color rgba="0.4 0.3 0.1 1"/>
   </material>
</robot>
